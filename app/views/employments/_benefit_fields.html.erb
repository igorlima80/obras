<div class="benefit nested-fields">
  <div class='row'>    
    <%= f.association :benefit_type, collection: BenefitType.order(:name), wrapper_html: { class: 'col-md-3' }  %>
    <%= f.input :value, wrapper_html: { class: 'col-md-3' } %>    

    <div class="col-md-2">
      <%= link_to_remove_association f, class: 'btn btn-danger top-32' do %>
        <i class="fa fa-fw fa-times"></i>
      <% end %>
    </div>
  </div>
</div>